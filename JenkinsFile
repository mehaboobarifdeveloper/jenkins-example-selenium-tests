pipeline {
  agent any
  stages {
    stage('Verify browsers are installed') {
      steps {
        sh 'google-chrome --version'
      }
    }
    stage('Run Tests') {
      steps {
       sh """
       # print current user, group
       id

       # print folder's user group/access
       ls -l "${mvnHome}"/..
       ls -l "${mvnHome}"

       "${mvnHome}"/bin/mvn --version
        sh './mvnw clean test'
      }
    }
  } 
}
